{
  "name": "X-Map 10",
  "description": "Chrome Application to connect X-Plane 10 with Google Maps",
  "version": "1.0",
  "manifest_version": 3,
  
  "app": 
    {
	"background": 
	{
	  "scripts": ["background.js"]
	}
    },
    
    "sandbox": 
    {
     "pages": ["sandbox.html"]
    },
  "icons": { "16": "images/icon-16.png", "32": "images/icon-32.png", "48": "images/icon-48.png", "128": "images/icon-128.png" },
  
  "sockets":
  {
    "udp": { "bind" : ":49003" }
  }
  
}